{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@homeComponents/*": ["src/home/components/*"],
      "@homeComponentsIslands/*": ["src/home/componentsIslands/*"],
      "@homeLayouts/*": ["src/home/layouts/*"],
      "@assets/*": ["src/assets/*"],
      "@product/*": ["./src/product/*"],
      "@utils/*": ["./src/utils/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
